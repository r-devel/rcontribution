                "block_id": "\/1cx",
                "elements": [
                    {
                        "type": "rich_text_section",
                        "elements": [
                            {
                                "type": "text",
                                "text": "It's not a bug, "
                            },
                            {
                                "type": "text",
                                "text": "old.packages",
                                "style": {
                                    "code": true
                                }
                            },
                            {
                                "type": "text",
                                "text": " will find any packages that are not up to date. You can specify a "
                            },
                            {
                                "type": "text",
                                "text": "lib.loc",
                                "style": {
                                    "code": true
                                }
                            },
                            {
                                "type": "text",
                                "text": " argument in "
                            },
                            {
                                "type": "text",
                                "text": "old.packages",
                                "style": {
                                    "code": true
                                }
                            },
                            {
                                "type": "text",
                                "text": " to get the list that you are after. The packages in "
