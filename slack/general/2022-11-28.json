                            {
                                "type": "text",
                                "text": "old.packages()",
                                "style": {
                                    "code": true
                                }
                            },
                            {
                                "type": "text",
                                "text": "the list of installed packages, sorted by version and removed the duplicated packages:\n"
                            }
                        ]
